{
  "name": "pinxit",
  "description": "Source code of pinxit.me",
  "repository": {
    "type": "git",
    "url": "git://github.com/mfpierre/pinxit.me.git"
  },
  "browser": {
    "fss": "./src/javascript/lib/fss.min.js",
    "jfp": "./src/javascript/lib/jquery-floating-placeholder.min.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "uglifyify"
    ]
  },
  "browserify-shim": {
    "fss": {
      "exports": "plugin"
    },
    "jfp": {
      "exports": "jfp",
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~0.8.2",
    "browserify": "~3.36.0",
    "browserify-shim": "~3.4.1",
    "coffeeify": "~0.6.0",
    "gulp": "^3.8.0",
    "gulp-changed": "^0.4.1",
    "gulp-imagemin": "^0.6.2",
    "gulp-notify": "^1.4.2",
    "gulp-util": "^3.0.0",
    "pretty-hrtime": "~0.2.1",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~0.10.1",
    "gulp-jade": "~0.7.0",
    "gulp-less": "~1.3.3",
    "uglifyify": "~2.5.0"
  },
  "dependencies": {
    "jquery": "~2.1.1"
  }
}
